{
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 80,
  "semi": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "quoteProps": "consistent",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "always",
  "embeddedLanguageFormatting": "auto"
}
